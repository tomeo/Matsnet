      <%= if @current_user do %>
      <div>
        <ul class="flex gap-6 tracking-tighter">
          <li>
            <.link
              href={~p"/tasks"}
            >
              Tasks
            </.link>
          </li>
          <li>
            <.link
              href={~p"/projects"}
            >
              Projects
            </.link>
          </li>
          <li>
            <.link
              href={~p"/roles"}
            >
              Roles
            </.link>
          </li>
          <li>
            <.link
              href={~p"/teams"}
            >
              Teams
            </.link>
          </li>
        </ul>
      </div>
      <% end %>
    </div>
  <%= if @current_user do %>
    <div>
      <ul class="flex items-center gap-4 justify-end text-black tracking-tighter">
        <li>
          <.link
            href={~p"/users/settings"}
          >
            Settings
          </.link>
        </li>
        <li>
          <.link
            href={~p"/users/log_out"}
            method="delete"
          >
            Log out
          </.link>
        </li>
      </ul>
    </div>
  <% else %>
    <div>
      <ul class="flex gap-6 tracking-tighter">
        <li>
          <.link
            href={~p"/users/register"}
          >
            Register
          </.link>
        </li>
        <li>
          <.link
            href={~p"/users/log_in"}
          >
            Log in
          </.link>
        </li>
      </ul>
    </div>
    <% end %>
